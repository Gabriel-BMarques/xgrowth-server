{
  "name": "welever-server",
  "version": "1.1.1",
  "description": "Welever server project",
  "author": "Andr√© Mello <andre@headfive.com>",
  "main": "src/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "scripts": {
    "postinstall": "npm install --save bluebird",
    "start": "node ./src/index.js",
    "demo": "cross-env NODE_ENV=demo pm2 start ./src/index.js -i max",
    "dev": "nodemon ./src/index.js",
    "lint": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --recursive src/api/tests",
    "test:unit": "cross-env NODE_ENV=test mocha src/api/tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 20000 src/api/tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --watch src/api/tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "validate": "yarn lint && yarn test",
    "postpublish": "git push --tags",
    "docs": "apidoc -i src -o docs",
    "postdocs": "opn docs/index.html",
    "docker:start": "cross-env NODE_ENV=production pm2-docker start ./src/index.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:danielfsousa/express-rest-es2017-boilerplate.git"
  },
  "keywords": [],
  "dependencies": {
    "@azure/storage-blob": "^12.2.0-preview.1",
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@ffprobe-installer/ffprobe": "^1.2.0",
    "@sendgrid/mail": "^6.4.0",
    "agenda": "^4.2.1",
    "axios": "^0.21.1",
    "azure-sb": "^0.11.1",
    "azure-storage": "^2.10.3",
    "bcryptjs": "2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.3",
    "cron": "^1.8.2",
    "cross-env": "^5.0.1",
    "csurf": "^1.11.0",
    "dotenv-safe": "^6.0.0",
    "express": "^4.17.1",
    "express-validation": "^1.0.2",
    "ffmpeg": "0.0.4",
    "ffprobe": "^1.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^4.4.1",
    "http-status": "^1.0.1",
    "ics": "^2.29.0",
    "into-stream": "^5.1.1",
    "jimp": "^0.16.1",
    "joi": "^10.4.1",
    "jquery": "^3.5.1",
    "jsdom": "^16.4.0",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.20",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.13",
    "mongoose": "^5.13.12",
    "morgan": "^1.8.1",
    "multer": "^1.4.2",
    "nanoid": "^2.0.3",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "4.0.0",
    "passport-saml": "^3.1.0",
    "pm2": "^5.1.0",
    "pngjs": "^5.0.0",
    "pngjs-image": "^0.11.7",
    "sharp": "^0.27.2",
    "stream": "0.0.2",
    "string-similarity": "^4.0.4",
    "uuid": "^3.1.0",
    "video-thumbnail-generator": "^1.0.8",
    "winston": "^3.2.1",
    "woopra": "^1.0.2",
    "xlsx": "^0.17.0",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "apidoc": "^0.17.7",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.14.3",
    "mocha": "^9.0.3",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "opn-cli": "^1.0.0",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.0.0",
    "supertest": "^3.0.0"
  }
}
